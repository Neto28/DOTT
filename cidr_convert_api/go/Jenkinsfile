pipeline {
  agent any
  tools {
    go 'go-1.15'
  }
  environment {
    GO151MODULE = 'on'
  }
  stages {
    stage('Build') {
      steps {
        echo 'BUILDING'
      }
    }

    stage('SonarCloud') {
      steps {
        echo 'SONARCLOUD STATIC CODE ANALYSIS'
        //sh 'sonar-scanner'
      }
    }

    stage('Testing') {
      steps {
        warnError(message: 'Failed to Run') {
            sh 'go version'
        }
      }
    }

    stage('Deploy') {
      steps {
        echo "Test"
      }
    }
  }
}
