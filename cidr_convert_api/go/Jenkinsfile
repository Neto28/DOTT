pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh '''echo "Testing Build Phase"
cd cidr_convert_api/go/
pwd 
ls
goop install'''
      }
    }

    stage('Test') {
      steps {
        sh '''echo "Testing Test Phase"
cd cidr_convert_api/go/
go test'''
      }
    }

    stage('Deploy') {
      steps {
        sh 'echo "Testing ipv4Validation"'
      }
    }

  }
}