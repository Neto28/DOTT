pipeline {
	agent any
		stages {
			stage('One') {
				steps {
					sh 'echo "Step One"'
					sh 'curl localhost:8000/cidr-to-mask?value=24'
				}
			}


			stage('Two') {
				steps {
					sh 'echo "Step Two"'
					sh 'curl localhost:8000/mask-to-cidr?value=255.255.0.0'
				}
			} 

			stage('Three') {
				steps {
					sh 'echo "Step Three"'
					sh 'curl localhost:8000/ip-validation?value=255.255.0.0'
				}
			}
		}
}

