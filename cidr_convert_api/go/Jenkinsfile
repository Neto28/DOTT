pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh '''echo "Testing Build Phase"
cd cidr_convert_api/go/
goop install'''
      }
    }

    stage('Test') {
      steps {
        sh '''echo "Testing Test Phase"
cd cidr_convert_api/go/

'''
      }
    }

    stage('Deploy') {
      steps {
        sh 'echo "Testing ipv4Validation"'
      }
    }

    stage('SonarCloud') {
      steps {
        sh '''echo "Test"
def scannerHome = tool \'SonarScanner 4.0\';
withSonarQubeEnv(\'My SonarQube Server\') {
sh "${scannerHome}/bin/sonar-scanner"
}'''
      }
    }

  }
}